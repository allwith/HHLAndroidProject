<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.hhl.hhlandroidproject" >

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".ui.activity.MainActivity"
            android:label="@string/app_name" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".ui.activity.AboutActivity"
            android:label="@string/title_activity_about"
            android:theme="@style/HHL.SwipeBack.Transparent.Theme" />
        <activity
            android:name=".ui.activity.MultiItemActivity"
            android:theme="@style/HHL.SwipeBack.Transparent.Theme" />
        <activity
            android:name=".ui.activity.SimpleItemActivity"
            android:label="@string/title_activity_simple_item" >
        </activity>
    </application>

</manifest>
